#include <iostream>
using namespace std;
int add(int a, int b) 
{
    return a + b;
}
float add(float a, float b) 
{
    return a + b;
}
int main() 
{
    int num1 = 5, num2 = 7;
    float num3 = 2.3, num4 = 5.8;
    cout << "Addition of integer: " << add(num1, num2) <<endl;
    cout << "Addition of floating-point : " << add(num3, num4) <<endl;
    return 0;
}
