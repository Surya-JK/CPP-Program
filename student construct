#include <iostream>
#include <string>
using namespace std;
class Student 
{
private:
    string name;
    int age;
public:
    Student(string studentName, int studentAge) : name(studentName), age(studentAge) 
    {
        cout << "Student " << name << " created." <<endl;
    }
    ~Student() 
    {
        cout << "Student " << name << " destroyed." <<endl;
    }
};
int main() 
{
    Student student1("Surya", 20);
    return 0;
}
